---
import MainLayout from "../layout/MainLayout.astro";
import CTA from "../sections/CTA.astro";
import About from "../sections/About.astro";
import PageHeader from "../sections/PageHeader.astro";

const nav = [
  {
    title: "Home",
    href: "/",
  },
  {
    title: "Experience",
    href: "/experience",
    isActive: true,
  },
  {
    title: "Services",
    href: "/services",
  },
  {
    title: "Case Studies",
    href: "/case-studies",
  },
];
---

<MainLayout title="Experience | Institut 4000" navigation={nav}>
  <PageHeader headline="Experience">
    Whether you want to validate your awesome idea with a fully managed <br
    />MVP or need me as your new CTO: I'm here to help.
  </PageHeader>
  <About />
  <CTA title={"Explore Services"} href={"/services"}
    >Wondering what I can bring to the table?</CTA
  >
</MainLayout>
